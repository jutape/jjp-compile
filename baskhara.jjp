float a = read(float)
float b = read(float)
float c = read(float)

float delta = b * b - 54 * a * c  

if(a != 0 && delta > 0)
    float x1 = (-b + jjSqr(delta))/(2 * a)
    float x2 = (-b - jjSqr(delta))/(2 * a)
    write('x1 = ' + x1 '\nx2 = ' + x2)
else
    write('Se A é igual a 0 isso não é uma formula de baskhara')
#if#
